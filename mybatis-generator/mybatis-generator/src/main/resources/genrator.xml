  <!--"E:\works\stsworkspace2\mybatis-generator\src\main\java\org\mybatis\generator\config\xml\mybatis-generator-config_1_0.dtd"-->
  <!-- E:\works\stsworkspace2\mybatis-generator\src\main\java\org\mybatis\generator\config\xml\mybatis-generator-config_1_0.dtd -->
<!-- http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd -->
<generatorConfiguration>
	<!-- classPathEntry:数据库的JDBC驱动的jar包地址 -->
	<!-- <classPathEntry location="mysql-connector-java-5.1.27.jar" /> -->

	<context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="conditional">
		<property name="javaFileEncoding" value="UTF-8"/>
		<property name="xmlFormatter" value="com.haogrgr.mybatis.generator.plugin.MyXmlFormatter"/>
		<!-- true 不覆盖进行叠加 false进行覆盖 -->
		<property name="mergeable" value="false"/>
	
		<plugin type="com.haogrgr.mybatis.generator.plugin.MyPlugin"></plugin>
		
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/cheanxin?autoReconnect=true&amp;useUnicode=true&amp;characterEncoding=utf-8"
			userId="root" password="root">
		</jdbcConnection>

		<!-- true:将DECIMAL,NUMERIC解析为BigDecimal false:解析为Integer -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="true" />
		</javaTypeResolver>

		<!-- 生成模型的包名和位置 -->
		<!-- <javaModelGenerator targetProject="F:/workspace/forum/src/main/java" targetPackage="com.kapphk.forum.bean"> -->
		<javaModelGenerator targetProject="D:\\ideaworkspace\\demo2\\src\\main\\java" targetPackage="com.example.demo.model">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>
		
		<!-- 生成service包名和位置  暂未实现-->
		<!--<javaServiceGenerator targetPackage="D:\\ideaworkspace\\demo2\\src\\main\\java" targetProject="com.example.demo.service">
			<property name="enableSubPackages" value="false" />
		</javaServiceGenerator>-->
		<!-- 生成Controller包名和位置  暂未实现-->
		<!--<javaControllerGenerator targetPackage="D:\\ideaworkspace\\demo2\\src\\main\\java" targetProject="com.example.demo.web">
			<property name="enableSubPackages" value="false" />
		</javaControllerGenerator>-->

		<!-- 生成的映射文件包名和位置 -->
		<sqlMapGenerator targetProject="D:\ideaworkspace\demo2\src\main\resources" targetPackage="mapper">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator type="XMLMAPPER" targetProject="D:\\ideaworkspace\\demo2\\src\\main\\java" targetPackage="com.example.demo.mapper">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
		
		

		<!-- tableName:用于自动生成代码的数据库表；domainObjectName:对应于数据库表的javaBean类名 -->
		
		  
		<table tableName="dept_city" domainObjectName="DeptCity"
			enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false"
			enableUpdateByExample="false" selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="JDBC" />
		</table>
	</context>
</generatorConfiguration> 